themes {
	content {
		# Variants for content elements
		variants {
			default {
				social = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.social
			}

			# Special variant for Header
			# Used by ctype header
			# Will be merged with default
			header {
				alert-dismissable = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.alert_dismissable
				infoboard-success = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.infoboard_success
				infoboard-info = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.infoboard_info
				infoboard-warning = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.infoboard_warning
				infoboard-danger = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.infoboard_danger
			}

			# ctype: text
			text {
				alert-dismissable = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.alert_dismissable
				infoboard-success = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.infoboard_success
				infoboard-info = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.infoboard_info
				infoboard-warning = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.infoboard_warning
				infoboard-danger = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.infoboard_danger
			}

			# ctype: gridelements_pi1
			gridelements_pi1 {
				tabs {
					navigation-pills-green = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_green
					navigation-pills-blue = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_blue
					navigation-pills-orange = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_orange
					navigation-pills-red = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.navigation_pills_red
				}
				container {
					alert-dismissable = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.alert_dismissable
					infoboard-success = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.infoboard_success
					infoboard-info = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.infoboard_info
					infoboard-warning = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.infoboard_warning
					infoboard-danger = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.infoboard_danger
				}
				singleColumnHeaderFooter {
					panel-green = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.panel_green
					panel-blue = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.panel_blue
					panel-orange = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.panel_orange
					panel-red = LLL:EXT:theme_bootstrap_mosaic/Resources/Private/Language/locallang.xlf:content.variants.panel_red
				}
			}
		}
	}
}