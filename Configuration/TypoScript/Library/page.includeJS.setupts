page {
	includeJSlibs {
		#modernizr = https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js
		modernizr = typo3conf/ext/theme_bootstrap_mosaic/Resources/Public/JavaScript/modernizr.min.js
                modernizr {
			external = 0
			forceOnTop = 1
			disableCompression = 1
			excludeFromConcatenation = 1
		}
	}
	includeJS {
		#bootstrap = http://netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js
		bootstrap = typo3conf/ext/theme_bootstrap_mosaic/Resources/Public/JavaScript/bootstrap.min.js
			bootstrap {
			external = 0
			disableCompression = 1
			excludeFromConcatenation = 1
		}
		theme = EXT:theme_bootstrap/Resources/Public/JavaScript/default.js
		theme {
			external = 0
			disableCompression = 1
			excludeFromConcatenation = 1
		}
                #JS Plugins
                scrolltopcontrol = EXT:theme_bootstrap_mosaic/Resources/Public/JavaScript/scrolltopcontrol.js
                sticky = EXT:theme_bootstrap_mosaic/Resources/Public/JavaScript/sticky.js
                #JS Custom
                custom = EXT:theme_bootstrap_mosaic/Resources/Public/JavaScript/custom.js
	}
}